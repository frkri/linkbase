		"fix:lint": "eslint . --fix"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.5",
		"@sveltejs/kit": "^2.5.28",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.7",
		"@types/eslint": "^9.6.1",
		"eslint": "^9.11.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.44.0",
		"globals": "^15.9.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "5.0.0-next.259",
		"svelte-check": "^4.0.2",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.7.0",
		"vite": "^5.4.8"
	},
	"type": "module"
}
