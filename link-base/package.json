{
	"name": "link-base",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"preview": "vite preview",
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && prettier --check . && eslint .",
